<% const nombreUsuario = nombre;%>
<%- include('menu', {username: nombreUsuario})%>
<!doctype html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Proyecto</title>
</head>
<body>
<table>
    <tr>
        <th>curso</th>
        <th>calificacion</th>
        <th>observacion</th>
    </tr>
    <%arrayNotas.forEach((notas)=>{%>
    <tr>
        <td><%=notas.cursoId.nombreCurso%></td>
        <td><%=notas.calificaciones%></td>
        <td><%=notas.observaciones%></td>
    </tr>
    <%})%>
</table>
</body>
</html>